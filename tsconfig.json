{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "stores/*": ["src/stores/*"],
      "pages/*": ["src/pages/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"]
    }
  },
  "skipLibCheck": true,
  "exclude": ["node_modules"]
}
